{
  "version": 2,
  "builds": [
    {
      "src": "chrome-extension.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/chrome-extension",
      "dest": "/chrome-extension.html"
    },
    {
      "src": "/",
      "dest": "/chrome-extension.html"
    }
  ]
}
